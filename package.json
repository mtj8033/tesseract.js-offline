{
  "name": "tesseract.js-offline",
  "version": "0.1.0",
  "description": "An example to show how to use tesseract.js in offline mode",
  "main": "index.js",
  "scripts": {
    "copyWorkerMin": "npx copyfiles node_modules/tesseract.js/dist/worker.min.js dist",
    "copyTessCore": "npx copyfiles node_modules/tesseract.js-core/tesseract-core.js dist",
    "start": "npm run copyWorkerMin && npm run copyTessCore && npx parcel browser/index.html --open --port 5678"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeromewu/tesseract.js-offline.git"
  },
  "author": "Jerome Wu <jeromewus@gmail.com> (https://github.com/jeromewu)",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/jeromewu/tesseract.js-offline/issues"
  },
  "homepage": "https://github.com/jeromewu/tesseract.js-offline#readme",
  "dependencies": {
    "highlight.js": "^9.15.8",
    "image-capture": "^0.4.0",
    "tesseract.js": "^2.0.0-alpha.10",
    "webrtc-adapter": "^7.2.4"
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "rimraf": "^2.6.3"
  }
}
